{
    "type": "module",
    "name": "splat3api-backend",
    "version": "1.0.0",
    "main": "dist/index.js",
    "author": "medi",
    "license": "MIT",
    "private": false,
    "scripts": {
        "dev": "node --loader ts-node/esm ./src/index.ts",
        "devfn": "functions-framework --target=index --signature-type=event",
        "build": "tsc",
        "deploy": "tsc && gcloud functions deploy index --runtime nodejs16 --trigger-topic=splat3",
        "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js tests/ --maxWorkers=1"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "^3.1.3",
        "@google-cloud/logging-winston": "^5.3.0",
        "dotenv": "^16.0.3",
        "nxapi": "^1.6.0",
        "redis": "^4.5.1",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/plugin-transform-modules-commonjs": "^7.20.11",
        "@babel/preset-env": "^7.20.2",
        "@babel/register": "^7.18.9",
        "@types/babel__register": "^7.17.0",
        "@types/jest": "^29.2.6",
        "@types/node": "^18.11.18",
        "@types/redis-mock": "^0.17.1",
        "babel-jest": "^29.3.1",
        "babel-plugin-transform-import-meta": "^2.2.0",
        "jest": "^29.3.1",
        "nodemon": "^2.0.20",
        "redis-mock": "^0.56.3",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    }
}
